{
  "name": "ethanede",
  "version": "1.0.0",
  "description": "Ethan Ede WordPress Theme",
  "scripts": {
    "scss": "sass assets/scss/main.scss assets/css/main.css",
    "tailwind": "npx tailwindcss -i ./assets/css/tailwind.css -o ./assets/css/tailwind-output.css --minify",
    "build": "npm run scss && npm run tailwind",
    "watch": "gulp watch"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.17",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^3.0.0",
    "postcss": "^8.4.35",
    "postcss-cli": "^11.0.0",
    "sass": "^1.71.1",
    "tailwindcss": "^3.4.1"
  }
}
